<?xml version="1.0" encoding="utf-8" ?>
<widget id="DatagridContextMenu.widget.DatagridContextMenu" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>DatagridContextMenu</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="datagridIdentifier" type="string" required="true" defaultValue="">
            <caption>DataGrid identifier</caption>
            <category>Settings</category>
            <description>Provide a unique datagrid identifier using css classes. E.g. ".mx-name-grid1"</description>
        </property>
        <property key="buttonsExclude" type="string" required="false" defaultValue="">
            <caption>Buttons to exclude</caption>
            <category>Settings</category>
            <description>The button(s) you wish to exclude, use the Mendix "Caption" and seperate list using ",". (optional)</description>
        </property>
    </properties>
</widget>
